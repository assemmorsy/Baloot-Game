{
  "kind": "collectionType",
  "collectionName": "studios",
  "info": {
    "singularName": "studio",
    "pluralName": "studios",
    "displayName": "استوديو",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "start_at": {
      "type": "datetime"
    },
